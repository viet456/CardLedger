    "install": "^0.13.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.544.0",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "p-limit": "^7.2.0",
