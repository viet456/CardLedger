    "db:backfill-release-date": "tsx scripts/backfillReleaseDate.ts",
    "db:backfill-tcg": "tsx scripts/backfillTcgPlayerSetIds.ts",
    "db:backfill-price": "tsx scripts/backfillPriceHistory.ts",
    "db:update-market": "tsx scripts/updateMarketStats.ts",
    "db:sync": "pnpm run db:populate && pnpm run images:migrate",
    "index:cards": "tsx scripts/generateCardIndex.ts",
    "index:market": "tsx scripts/generateMarketIndex.ts",
    "optimize:images": "tsx scripts/optimizeImages.ts"
  },
  "dependencies": {
