    "@tsconfig/recommended": "^1.0.10",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
